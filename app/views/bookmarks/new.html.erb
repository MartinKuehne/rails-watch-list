<h1>Create a new bookmark</h1>

<%= simple_form_for [ @list, @bookmark ] do |f| %>
  <%= f.input :movie_id,:collection => @movies,:label_method => :title,:value_method => :id,:label => "Title" ,:include_blank => false %>
  <%= f.input :comment %>
  <%= f.submit "Create new bookmark", class: "btn btn-primary" %>
<% end %>